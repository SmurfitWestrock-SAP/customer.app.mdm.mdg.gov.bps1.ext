  <!-- enabled="{ModelVue>/enable}" -->
  <core:FragmentDefinition id="IdUPTDialogFrag" xmlns:core='sap.ui.core' xmlns='sap.m' xmlns:form="sap.ui.layout.form"
      xmlns:smartForm="sap.ui.comp.smartform" xmlns:smartfield="sap.ui.comp.smartfield"
      core:require="{Time: 'sap/ui/model/odata/type/Time'}">
      <Dialog id="IdUPTDialog" showHeader="false">
          <customHeader>
              <Toolbar>
                  <Label text="{i18n>read.title}" design="Bold" />
                  <ToolbarSpacer />
                  <Button id="IdUPTCancel" icon="sap-icon://sys-cancel" type="Reject" press="onDialogClose" />
              </Toolbar>
          </customHeader>
          <content>
              <smartForm:SmartForm id="IdUnloadPointSF" editable="true">
                  <smartForm:Group>
                      <smartForm:GroupElement>
                          <smartfield:SmartField value="{UnloadingPointName}" enabled="{=${UpdNotEnabled} ? false:true}" />
                      </smartForm:GroupElement>
                      <smartForm:GroupElement>
                          <smartfield:SmartField value="{IsDfltBPUnloadingPoint}" />
                      </smartForm:GroupElement>
                      <smartForm:GroupElement>
                          <smartfield:SmartField value="{CustomerFactoryCalenderCode}" />
                      </smartForm:GroupElement>
                      <smartForm:GroupElement>
                          <smartfield:SmartField value="{BPGoodsReceivingHoursCode}" change="onGRHoursCodeChg"/>
                      </smartForm:GroupElement>                      
                      <smartForm:GroupElement>
                          <smartfield:SmartField
                              value="{path: 'MondayMorningOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'MondayMorningClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'MondayAfternoonOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'MondayAfternoonClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                      </smartForm:GroupElement>
                      <smartForm:GroupElement>
                          <smartfield:SmartField
                              value="{path: 'TuesdayMorningOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'TuesdayMorningClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'TuesdayAfternoonOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'TuesdayAfternoonClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                      </smartForm:GroupElement>
                      <smartForm:GroupElement>
                          <smartfield:SmartField
                              value="{path: 'ThursdayMorningOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'ThursdayMorningClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'ThursdayAfternoonOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'ThursdayAfternoonClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                      </smartForm:GroupElement>
                      <smartForm:GroupElement>
                          <smartfield:SmartField
                              value="{path: 'WednesdayAfternoonClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'WednesdayAfternoonOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'WednesdayMorningClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'WednesdayMorningOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                      </smartForm:GroupElement>
                      <smartForm:GroupElement>
                          <smartfield:SmartField
                              value="{path: 'FridayMorningOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'FridayMorningClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'FridayAfternoonOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'FridayAfternoonClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                      </smartForm:GroupElement>
                      <smartForm:GroupElement>
                          <smartfield:SmartField
                              value="{path: 'SaturdayMorningOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'SaturdayMorningClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'SaturdayAfternoonOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'SaturdayAfternoonClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                      </smartForm:GroupElement>
                      <smartForm:GroupElement>
                          <smartfield:SmartField
                              value="{path: 'SundayMorningOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'SundayMorningClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'SundayAfternoonOpeningTime', type: 'Time', formatOptions: { style: 'short' }}" />
                          <smartfield:SmartField
                              value="{path: 'SundayAfternoonClosingTime', type: 'Time', formatOptions: { style: 'short' }}" />
                      </smartForm:GroupElement>
                  </smartForm:Group>
              </smartForm:SmartForm>
          </content>
          <footer>
              <Toolbar id="IdUPTBar">
                  <Button id="IdUPTPopMsg" icon="sap-icon://message-popup" press="handleMessagePopoverPress"
                      type="{= (${message>/}.length > 0 ? 'Negative':'Default')}" text="{=${message>/}.length}" />
                  <ToolbarSpacer />
                  <Button id="IdUPTVal" text="{i18n>bktyp.valid}" icon="sap-icon://accept" type="Accept"
                      press="onUPTSave" />
              </Toolbar>
          </footer>
      </Dialog>
  </core:FragmentDefinition>